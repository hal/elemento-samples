{
  "name": "elemento-samples",
  "description": "Elemento Samples",
  "private": true,
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "parcelIgnore": [
    "crysknife\/.*", "gwt\/.*", "j2cl\/.*"
  ],
  "source": "src/web/index.html",
  "scripts": {
    "watch": "parcel serve --public-url \"./\" --dist-dir target/samples --open",
    "build": "parcel build --public-url \"./\" --dist-dir target/samples"
  },
  "devDependencies": {
    "@parcel/reporter-cli": "^2.10.1",
    "parcel": "^2.9.3",
    "parcel-resolver-ignore": "^2.1.5"
  },
  "dependencies": {
    "todomvc-app-css": "^2.4.2",
    "todomvc-common": "^1.0.5"
  }
}
